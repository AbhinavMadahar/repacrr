#!/usr/bin/env sh

# place all environment variables here and nothing else

export CUDA_VISIBLE_DEVICES=0
export PATH="$PATH:/home/$USER/workspace/pyenv/anaconda3TFCPU/bin"
export LD_LIBRARY_PATH="/home/$USER/workspace/pyenv/cuDNN/cuda/lib64"

export expname=pacrrpub
export train_years=wt09_10
export numneg=6
export batch=32
export winlen=3
export kmaxpool=3
export distill="firstk"
export binmat=False
export context=False
export cascade=''
export combine=16
export qproximity=0
export iterations=10
export shuffle=False

if [ -d "/local/var/$USER/pacrr" ]; then
	parentdir="/local/var/$USER/pacrr"
else
	printf "parentdir: "
	read parentdir
fi

export parentdir
export outdir=$parentdir

printf "Directory of the similarity matrices: "
read sim_dir
export sim_dir
